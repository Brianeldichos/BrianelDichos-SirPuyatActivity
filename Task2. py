class Student:
    def __init__(self, name, age, course):
        self.name = name
        self.age = age
        self.course = course

    def introduce(self):
        print(f"Hi, I'm {self.name}, {self.age} years old, and I'm studying {self.course}.")


student1 = Student("Brianel Dichos", 23, "Information Technology")
student2 = Student("Yanyan Dichos", 18, "Nursing")
student3 = Student("Yanie Dichos", 23, "Culinary")


student1.introduce()
student2.introduce()
student3.introduce()
